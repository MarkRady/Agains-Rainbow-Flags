{
  "name": "Against Rainbow",
  "description": "I Am Not Support Rainbow",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "I am not support the Rainbow.",
    "default_icon": "images/icon.png"
  },
  "manifest_version": 2,
  "web_accessible_resources" : [ 
    "images/*.png" 
  ]
}
